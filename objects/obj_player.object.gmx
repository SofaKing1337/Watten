            <string>/// Player Movement
/* When not moving, check to see if a direction key is pressed.
   If so, assign x/y speed and change status to moving. */
if (speedX == 0 &amp;&amp; speedY == 0){
  if (keyboard_check_pressed(vk_right)){
    speedX = moveSpeed;
  } 
  if (keyboard_check_pressed(vk_left)){
    speedX = -moveSpeed;
  }
  if (keyboard_check_pressed(vk_down)){
    speedY = moveSpeed;
  } 
  if (keyboard_check_pressed(vk_up)){
    speedY = -moveSpeed;
  }
  if (speedX != 0 || speedY != 0){
    moveTimer = gridSize;
  }
} else {
   /* When moving, subtract from moveTimer our moveSpeed value
      and update location relevant to set speeds.
      Stop moving when moveTimer reaches zero. */
  x += speedX;
  y += speedY;

  moveTimer -= moveSpeed;
  if (moveTimer == 0) {
    speedX = 0;
    speedY = 0;
  }
}


